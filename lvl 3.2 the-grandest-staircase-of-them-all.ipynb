    "def solution(n):\n",
    "\n",
    "    staircase = [1] + [0]* n\n",
    "\n",
    "    for i in range(1, n+1):\n",
    "        for j in range(n, i-1, -1):\n",
    "            staircase[j] += staircase[j-i]\n",
    "    return staircase[n] - 1\n",
    "print solution(5), solution(200)"
   ]
